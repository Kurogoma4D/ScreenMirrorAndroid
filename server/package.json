{
    "name": "screen-mirror-app",
    "version": "0.0.1",
    "description": "",
    "main": "dist/main.js",
    "scripts": {
        "start": "webpack --mode=development",
        "watch": "webpack -w --mode=development",
        "app": "electron .",
        "deploy": "set NODE_ENV=production && webpack",
        "package-all": "npm run deploy && electron-packager . ScreenMirrorApp --out=build --platform=all --arch=x64 --electron-version=6.0.10 --prune --overwrite --asar --ignore=build --ignore=src",
        "package-win": "npm run deploy && electron-packager . ScreenMirrorApp --out=build --platform=win32 --arch=x64 --electron-version=6.0.10 --prune --overwrite --asar --ignore=build --ignore=src",
        "package-mac": "npm run deploy && electron-packager . ScreenMirrorApp --out=build --platform=darwin --arch=x64 --electron-version=6.0.10 --prune --overwrite --asar --ignore=build --ignore=src",
        "package-linux": "npm run deploy && electron-packager . ScreenMirrorApp --out=build --platform=linux --arch=x64 --electron-version=6.0.10 --prune --overwrite --asar --ignore=build --ignore=src",
        "server": "node --experimental-modules server.mjs",
        "ngrok": "ngrok http 8080"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bootstrap": "^4.3.1",
        "jquery": "^3.4.1",
        "jquery.qrcode": "^1.0.3",
        "popper.js": "^1.15.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-transform-runtime": "^7.6.0",
        "@babel/preset-env": "^7.6.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.6.0",
        "@babel/runtime": "^7.6.0",
        "ajax": "0.0.4",
        "autoprefixer": "^9.6.1",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "bufferutil": "^4.0.1",
        "copy-webpack-plugin": "^5.0.4",
        "cors": "^2.8.5",
        "electron": "^6.0.10",
        "electron-packager": "^14.0.6",
        "express": "^4.17.1",
        "fs": "0.0.1-security",
        "html-webpack-plugin": "^3.2.0",
        "log4js": "^5.1.0",
        "npm": "^6.11.3",
        "ts-loader": "^6.1.2",
        "typescript": "^3.6.3",
        "utf-8-validate": "^5.0.2",
        "webpack": "^4.40.2",
        "webpack-cli": "^3.3.9",
        "ws": "^7.1.2"
    }
}
